<manifest package="com.baldev.eventify"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".presentation.createuser.CreateUserActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".presentation.mainactivity.MainActivity"/>
        <activity android:name=".presentation.creategroup.CreateGroupActivity"/>
        <activity android:name=".presentation.userlist.UserListActivity"/>

    </application>

</manifest>
